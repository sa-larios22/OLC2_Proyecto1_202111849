// ------------------------------------------------------------

print("********** Operaciones aritmeticas **********");

print("1. Suma");

print(10 + 5); // 15
print(10.5 + 5.5); // 16.0
print(10 + 5.5); // 15.5
print(10.5 + 5); // 15.5
print(numero5+numero6); // 30.5
print("");

print("2. Resta");
print(10 - 5); // 5
print(10.5 - 5.5); // 5.0
print(10 - 5.5); // 4.5
print(10.5 - 5); // 5.5
print(numero5-numero6); // -10.5
print("");

print("3. Multiplicacion");
print(10 * 5); // 50
print(10.5 * 5.5); // 57.75
print(10 * 5.5); // 55.0
print(10.5 * 5); // 52.5
print(numero5*numero6); // 205.0
print("");

print("4. Division");
print(10 / 5); // 2
print(10.5 / 5.5); // 1.9090909090909092
print(10 / 5.5); // 1.8181818181818181
print(10.5 / 5); // 2.1
print(numero5/numero6); // 0.4878048780487805
print("");

print("5. Modulo");
print(10 % 5); // 0
print(10 % 3); // 1
print("");

print("6. Resta unaria");
print(-10); // -10
print(-10.5); // -10.5
print("");

print("7. Compuesta");
print(-2 + (10 + 5) * 2 / 3 + 5 % 2); // 9
print("");

// ------------------------------------------------------------

print("********** Operaciones Relacionales **********");

print("1. Igualdad");
print(10 == 10); // true
print(10 == 5); // false
print(10.5 == 10.5); // true
print(10.5 == 5.5); // false
print(numero5 == numero6); // false
print("");

print("2. Diferencia");
print(10 != 10); // false
print(10 != 5); // true
print(10.5 != 10.5); // false
print(10.5 != 5.5); // true
print(numero5 != numero6); // true
print("");

print("3. Mayor que");
print(10 > 5); // true
print(10 > 10); // false
print(10.5 > 5.5); // true
print(10.5 > 10.5); // false
print(numero5 > numero6); // false
print("");

print("4. Menor que");
print(10 < 5); // false
print(10 < 10); // false
print(10.5 < 5.5); // false
print(10.5 < 10.5); // false
print(numero5 < numero6); // true
print("");

print("5. Mayor o igual que");
print(10 >= 5); // true
print(10 >= 10); // true
print(10.5 >= 5.5); // true
print(10.5 >= 10.5); // true
print(numero5 >= numero6); // false
print("");

print("6. Menor o igual que");
print(10 <= 5); // false
print(10 <= 10); // true
print(10.5 <= 5.5); // false
print(10.5 <= 10.5); // true
print(numero5 <= numero6); // true
print("");

// ------------------------------------------------------------
print("********** Operaciones Logicas **********");

print("1. AND");
print(true && true); // true
print(true && false); // false
print(false && true); // false
print(false && false); // false
print(10 == 10 && 5 == 5); // true
print(10 == 10 && 5 == 6); // false
print("");

print("2. OR");
print(true || true); // true
print(true || false); // true
print(false || true); // true
print(false || false); // false
print(10 == 10 || 5 == 5); // true
print(10 == 10 || 5 == 6); // true
print(10 == 11 || 5 == 6); // false
print("");

print("3. NOT");
print(!true); // false
print(!false); // true
print(!(10 == 10)); // false
print(!(10 == 11)); // true
print("");

