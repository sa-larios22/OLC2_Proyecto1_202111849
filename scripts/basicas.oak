print("********** Declaracion de variables **********");

print("1. Declaracion con tipo y valor");

int numero1 = 10;
float numero2 = 20.5;
string cadena = "Hola mundo";
char letra = 'a';

print(numero1);
print(numero2);
print(cadena);
print(letra);
print("");

print("2. Declaracion con tipo y sin valor");

int numero3; // null
float numero4; // null

print(numero3);
print(numero4);
print("");

print("3. Declaracion infiriendo el tipo");

var numero5 = 10; // tipo: int
var numero6 = 20.5; // tipo: float
var cadena2 = "Hola mundo"; // tipo: string
var letra2 = 'a'; // tipo: char

print(numero5);
print(numero6);
print(cadena2);
print(letra2);
print("");

